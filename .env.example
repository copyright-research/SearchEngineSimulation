# Google Custom Search API
GOOGLE_API_KEY=your_google_api_key
GOOGLE_SEARCH_ENGINE_ID=your_search_engine_id

# Tavily API (optional, for enhanced search)
TAVILY_API_KEY=your_tavily_api_key

# Groq API (optional, for alternative AI)
GROQ_API_KEY=your_groq_api_key

# Google AI API (for Gemini)
GOOGLE_GENERATIVE_AI_API_KEY=your_google_ai_api_key

# Database (Vercel Postgres or local Postgres)
# For Vercel: This will be auto-populated by Vercel Postgres
# For local: Use postgresql://user:password@localhost:5432/dbname
DATABASE_URL="postgresql://localhost:5432/copyright?schema=public"

# Rate Limiting Secret (optional, for production)
CRON_SECRET=your_secret_key
